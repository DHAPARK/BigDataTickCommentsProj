<mapper namespace="Chart" >

	
	<select id="getBoardCnt" resultType="_int">
		SELECT count(*) FROM TBL_BOARD
	</select>
	
	<select id="getDetail" parameterType="_int" resultType="boarddto">
		SELECT * FROM TBL_BOARD tb where boardnum = #{boardnum}
	</select>
	
	
</mapper>



























